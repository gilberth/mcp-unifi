# Variables de entorno para el servidor MCP UniFi
# Copia este archivo a .env y configura tus valores

# Configuración del controlador UniFi
UNIFI_ROUTER_IP=192.168.1.1
UNIFI_USERNAME=admin
UNIFI_PASSWORD=tu_password_aqui
UNIFI_PORT=443
UNIFI_VERIFY_SSL=false

# Configuración de la API
UNIFI_API_TIMEOUT=30

# Configuración de logging (opcional)
LOG_LEVEL=INFO